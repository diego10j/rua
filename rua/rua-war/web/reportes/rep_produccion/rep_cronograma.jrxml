	<queryString>
		<![CDATA[select a.ide_prpro, nombre_cndfp, identificac_geper, nom_geper,direccion_geper,telefono_geper, d.primer_nombre_gtemp||' '||d.apellido_paterno_gtemp as nombre_empleado,detalle_gtcar,
detalle_prvat as validez, numero_prpro,fecha_prpro,solitado_por_prpro,subtotal_prpro,valor_descuento_prpro,por_descuento_prpro,iva_prpro,total_prpro,observacion_prpro,
nombre_inuni,nombre_inarti,cantidad_prdep,valor_unitario_prdep,valor_total_prdep,subtotal_prpro-valor_descuento_prpro as subtotaldescuento,detalle_prvat as tiempo,
(select fecha_registro_gemos from gen_modulo_secuencial where activo_gemos = true  and ide_gemod = $P{pide_modulo}) as fecha_modulo
from prod_proforma a
left join con_deta_forma_pago b on a.ide_cndfp = b.ide_cndfp
left join gen_persona c on a.ide_geper = c.ide_geper
left join gth_empleado d on a.ide_gtemp = d.ide_gtemp
left join gth_cargo e on a.ide_gtcar = e.ide_gtcar
left join prod_validez_tiempo f on a.ide_prvat = f.ide_prvat
left join prod_detalle_proforma g on a.ide_prpro = g.ide_prpro
left join inv_unidad h on g.ide_inuni = h.ide_inuni
left join inv_articulo i on g.ide_inarti = i.ide_inarti
where a.ide_prpro = $P{ide_prpro}]]>
	</queryString>
