	<queryString>
		<![CDATA[select  ide_acafi, ide_acdepr  ,ide_accla,nombre_accla, valor_compra_acafi, valor_reposicion_acafi, valor_acdepr, valor_compra_acafi+valor_reposicion_acafi-valor_acdepr as valor_residual
from (
select b.ide_acdepr, a.ide_acafi, c.ide_accla, c.nombre_accla, b.valor_acdepr, a.valor_compra_acafi, a.valor_reposicion_acafi from act_activo_fijo a
left join act_depreciacion b on a.ide_acafi = b.ide_acafi
left join act_clase_activo c on a.ide_accla = c.ide_accla
