		<![CDATA[select a.observacion_acafi, a.ide_acafi, c.ide_accla, c.nombre_accla, b.valor_acdepr, a.valor_compra_acafi, a.fecha_compra_acafi, a.codigo_barras_acafi, a.valor_reposicion_acafi
,valor_compra_acafi+valor_reposicion_acafi-valor_acdepr as valor_residual
from act_activo_fijo a
left join (select ide_acafi,sum(valor_acdepr) as valor_acdepr from act_depreciacion where fecha_acdepr <= cast ($P{fecha_ingresar} as date) group by  ide_acafi ) b on a.ide_acafi = b.ide_acafi
left join act_clase_activo c on a.ide_accla = c.ide_accla
