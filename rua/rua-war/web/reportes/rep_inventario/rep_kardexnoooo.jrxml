	<queryString>
		<![CDATA[SELECT
     inv_det_comp_inve.PRECIO_INDCI AS inv_det_comp_inve_PRECIO_INDCI,
     inv_det_comp_inve.VALOR_INDCI AS inv_det_comp_inve_VALOR_INDCI,
     inv_det_comp_inve.CANTIDAD_INDCI AS inv_det_comp_inve_CANTIDAD_INDC,
     inv_bodega.NOMBRE_INBOD AS inv_bodega_NOMBRE_INBOD,
     inv_articulo.nombre_inarti AS inv_articulo_nombre_inarti,
     inv_tip_tran_inve.NOMBRE_INTTI AS inv_tip_tran_inve_NOMBRE_INTT,
     inv_unidad.nombre_inuni AS inv_unidad_nombre_inuni,
     inv_cab_comp_inve.NUMERO_INCCI AS inv_cab_comp_inve_NUMERO_INCCI,
     inv_cab_comp_inve.FECHA_TRANS_INCCI AS inv_cab_comp_inve_FECHA_TRANS_I,     
     inv_articulo.ide_inarti AS inv_articulo_ide_inarti,
     inv_articulo.observacion_inarti AS inv_articulo_observacion_inarti,
     inv_tip_comp_inve.SIGNO_INTCI AS inv_tip_comp_inve_SIGNO_INTCI
FROM
     inv_unidad INNER JOIN  inv_articulo ON inv_unidad.ide_inuni = inv_articulo.ide_inuni
     INNER JOIN inv_det_comp_inve ON inv_articulo.ide_inarti = inv_det_comp_inve.ide_inarti
     INNER JOIN inv_cab_comp_inve ON inv_det_comp_inve.ide_incci = inv_cab_comp_inve.ide_incci
     INNER JOIN inv_bodega ON inv_cab_comp_inve.ide_inbod = inv_bodega.ide_inbod
     INNER JOIN inv_tip_tran_inve ON inv_cab_comp_inve.ide_intti = inv_tip_tran_inve.ide_intti
     INNER JOIN inv_tip_comp_inve ON inv_tip_tran_inve.ide_intci = inv_tip_comp_inve.ide_intci
WHERE
inv_cab_comp_inve.FECHA_TRANS_INCCI BETWEEN '2011-01-01'AND '2012-12-31'
ORDER BY
     inv_bodega.NOMBRE_INBOD ASC,
     inv_articulo.nombre_inarti ASC,
     inv_tip_tran_inve.NOMBRE_INTTI ASC,
     inv_cab_comp_inve.FECHA_TRANS_INCCI ASC,
     inv_cab_comp_inve.NUMERO_INCCI ASC]]>
	</queryString>
